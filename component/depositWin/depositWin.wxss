.win-wraper {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 10;
}

.win-mask {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    background-color: rgba(0,0,0,0.6);
}

.win-cont {
    position: absolute;
    width: 560rpx;
    height: 644rpx;
    top: 50%;
    left: 50%;
    padding: 200rpx 30rpx 30rpx;
    margin-left: -280rpx;
    margin-top: -370rpx;
    background-color: #fff;
    border-radius: 10rpx;
}

.win-head-pic {
    position: absolute;
    width: 264rpx;
    height: 256rpx;
    top: -90rpx;
    left: 50%;
    margin-left: -132rpx;
}

.win-head-close {
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    width: 50rpx;
    height: 50rpx;
}

.select-item { padding: 20rpx 0;}

.select-item .item {
    line-height: 76rpx;
}
.select-item  {}

.select-item .ico-col-select { display: none;}
.select-item .active .ico-col-select { display: inline-block;}

